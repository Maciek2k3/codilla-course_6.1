CREATE TABLE ISSUELIST
(
   ID   SERIAL PRIMARY KEY,
   NAME VARCHAR(100)
);

CREATE TABLE ISSUES
(
   ID  SERIAL PRIMARY KEY,
   ISSUE_ID  BIGINT UNSIGNED NOT NULL,
   SUMMARY  VARCHAR(512),
   DESCRIPTION VARCHAR(1024),
   FOREIGN KEY (ISSUE_ID) REFERENCES ISSUELIST(ID)
);
INSERT INTO ISSUELIST (NAME)
VALUES ("ToDo");
INSERT INTO ISSUELIST (NAME)
VALUES ("Done");
INSERT INTO ISSUELIST (NAME)
VALUES ("InProgress");
